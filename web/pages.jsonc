        },
        {
            "name": "Configuration",
            "url": "/config",
            // "style": "pointer-events: none; cursor: default; color: gray; text-decoration: none; opacity: 0.6;"
        },
        {
            "name": "Settings",
            "url": "/settings",
            "style": "pointer-events: none; cursor: default; color: gray; text-decoration: none; opacity: 0.6;"
        },
        {
            "name": "Admin Page",
            "url": "/admin",
            "id": "admin",
            "style": "visibility: hidden; width: 0; text-wrap: nowrap; margin: 0; padding: 0; display: none;"
        },
        {
            "name": "About",
            "url": "/about",
            "id": "about",
