// Defines the routes used by the Webserver
{
    "paths": {
        "get": {
            "/": {
                "file": "home",
                "scripts": ["variables"],
                "settings": {
                    "stylesheets": [],
                    "scripts": [],
                    "widgets": [],
                    "loadbootstrap": true
                },
                "restriction": false
            },
            "/ejs-test": {
                "file": "test",
                "scripts": ["variables"],
                "settings": {
                    "type": "ejs",
                    "stylesheets": [],
                    "scripts": [],
                    "widgets": [],
                    "loadbootstrap": true
                },
                "restriction": false
            },
            "/about": {
                "file": "about",
                "scripts": ["variables"],
                "settings": {
                    "stylesheets": [],
                    "scripts": [],
                    "widgets": [],
                    "loadbootstrap": true
                },
                "restriction": false
            },
            "/:type(*)/:id(\\d+)/toggle": {
                "file": null,
                "scripts": ["toggle"],
                "restriction": false
            },
            "/proxy": {
                "file": "proxy",
                "scripts": ["variables", "confContext"],
                "settings": {
                    "stylesheets": [],
                    "scripts": ["reloadUtil.js"],
                    "widgets": [],
                    "loadbootstrap": true
                },
                "restriction": false
            },
            // "/proxy/*/toggle": {
            //     "file": null,
            //     "scripts": ["toggle"],
            //     "restriction": false
            // },
            "/proxy/*": {
                "file": "proxyEdit",
                // variables: add package values
                // confContext: add config values
                // urlContext: add url parsed params
                // noDetails: redirect from any/<int> back to the overview
                "scripts": ["variables", "confContext", "urlContext", "noDetails"],
                "settings": {
                    "stylesheets": [],
                    "scripts": [],
                    "widgets": [],
                    "loadbootstrap": true
                },
                "restriction": false
            },
            "/stub": {
                "file": "stubs",
                "scripts": ["variables", "confContext"],
                "settings": {
                    "stylesheets": [],
                    "scripts": ["reloadUtil.js"],
                    "widgets": [],
                    "loadbootstrap": true
                },
                "restriction": false
            },
            // "/stub/*/toggle": {
            //     "file": null,
            //     "scripts": ["toggle"],
            //     "restriction": false
            // },
            "/stub/*": {
                "file": "stubEdit",
                "scripts": ["variables", "confContext", "urlContext", "noDetails"],
                "settings": {
                    "stylesheets": [],
                    "scripts": [],
                    "widgets": [],
                    "loadbootstrap": true
                },
                "restriction": false
            },
            "/acme": {
                "file": "acme",
                "scripts": ["variables", "confContext"],
                "settings": {
                    "type": "handlebars",
                    "stylesheets": [],
                    "scripts": [],
                    "widgets": [],
                    "loadbootstrap": true
                },
                "restriction": false
            },
            "/config": {
                "file": null,
                "scripts": ["config"],
                "restriction": false
            },
            // "/admin": {
            //     "file": "admin",
            //     "scripts": ["admin"],
            //     "settings": {
            //         "subtitle": "Administration",
            //         "headerTitle": "Administrator Dashboard"
            //     },
            //     "restriction": ["admin"]
            // },
            // "/admin/*": {
            //     "file": "serveradmin",
            //     "scripts": ["serveradmin"],
            //     "settings": {
            //         "headerTitle": "Administrator Dashboard"
            //     },
            //     "restriction": ["admin"]
            // },
        },
        "post": {
            "/:type(*)/:id(\\d+)/delete": {
                "file": null,
                "scripts": ["delete"],
                "restriction": false
            },
            "/:type(*)/:id(\\d+|new)": {
                "file": null,
                "scripts": ["update"],
                "restriction": false
            },
            "/config/apply": {
                "file": null,
                "scripts": ["restartProxy"],
                "restriction": false
            },
            "/acme": {
                "file": null,
                "scripts": ["acme"],
                "restriction": false
            },
            // "/register": {
            //     "file": null,
            //     "scripts": ["register"],
            //     "restriction": false
            // },
            // "/admin/*": {
            //     "file": null,
            //     "scripts": ["serveradmin"],
            //     "restriction": ["admin"]
            // }
        }
