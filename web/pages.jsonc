    "navpages": [
        {
            "name": "Home",
            "url": "/"
        },
        {
            "name": "Proxy",
            "url": "/proxy"
        },
        {
            "name": "Redirect-Hosts",
            "url": "/redirects",
            "style": "pointer-events: none; cursor: default; color: gray; text-decoration: none; opacity: 0.6;"
        },
        {
            "name": "Port-Forwards",
            "url": "/port-forwards",
            "style": "pointer-events: none; cursor: default; color: gray; text-decoration: none; opacity: 0.6;"
        },
        {
            "name": "Stubs",
            "url": "/stub",
            // "style": "pointer-events: none; cursor: default; color: gray; text-decoration: none; opacity: 0.6;"
        },
        {
            "name": "Certificates",
            "url": "/acme",
